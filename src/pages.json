{
  "easycom": {
    "^i-(.*)": "@/components/i-$1/i-$1.vue",
    "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue",
    "^uni-(.*)": "@/uni-ui/components/uni-$1/uni-$1.vue",
    "^muni-(.*)": "@/muni/components/muni-$1/muni-$1.vue"
