{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
    "erp-(.*)": "@/components/erp-$1/erp-$1.vue"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/PersonalCenter/PersonalCenter",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/PersonalCenter/settings",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/goods/list",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/store/store",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/Login/Login",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/test/test",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/goods/create",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/goods/edit",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": true,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/put_order/create",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": true,
        "navigationStyle": "custom"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#333333",
    "selectedColor": "#1A97FF",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/erp/tabbar/home.png",
        "selectedIconPath": "static/erp/tabbar/home-active.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/goods/list",
        "iconPath": "static/erp/tabbar/goods.png",
        "selectedIconPath": "static/erp/tabbar/goods-active.png",
        "text": "货品管理"
      },
      {
        "pagePath": "pages/store/store",
        "iconPath": "static/erp/tabbar/store.png",
        "selectedIconPath": "static/erp/tabbar/store-active.png",
        "text": "库存管理"
      },
      {
        "pagePath": "pages/PersonalCenter/PersonalCenter",
        "iconPath": "static/erp/tabbar/user.png",
        "selectedIconPath": "static/erp/tabbar/user-active.png",
        "text": "我的"
      }
    ]
  }
}
